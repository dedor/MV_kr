	<beaconType id="map_icon_osmia" global="true">
		<color r="255" g="85" b="75" a="1.0"/>
		<unvisitedTooltip>Location of the terrorists.</unvisitedTooltip>
		<visitedTooltip>Explored location. Nothing left of interest.</visitedTooltip>
	</beaconType>
	<preventQuest/>
	<preventFleet/>
